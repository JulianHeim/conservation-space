###  http://www.sirma.com/ontologies/2014/03/eQMS#eQMSObject

:eQMSObject rdf:type owl:Class ;
            
            rdfs:label "QMS object"@en ;
            
            rdfs:subClassOf emf:DomainObject ;
            
            emf:canCreateInstance "false"^^xsd:boolean ;
            
